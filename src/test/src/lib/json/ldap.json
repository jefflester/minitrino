{
  "tests": [
    {
      "type": "query",
      "sql": "SELECT 12345",
      "trinoCliArgs": [
        "--server 'https://trino:8443'",
        "--user admin",
        "--truststore-path /etc/starburst/ssl/truststore.jks",
        "--truststore-password changeit",
        "--password"
      ],
      "successCriteria": {
        "contains": [
          "12345"
        ]
      },
      "env": {
        "TRINO_PASSWORD": "trinoRocks15"
      }
    },
    {
      "type": "logs",
      "container": "trino",
      "successCriteria": {
        "contains": [
          "=== SERVER STARTED ==="
        ]
      },
      "timeout": 60
    }
  ]
}