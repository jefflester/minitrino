{
   "catalogs":[
      {
         "group":"platform-admins",
         "catalog":".*",
         "allow":"all"
      },
      {
         "group":"metadata-users|platform-users",
         "catalog":"system",
         "allow":"all"
      },
      {
         "catalog":"system",
         "allow":"none"
      },
      {
         "catalog":".*",
         "allow":"none"
      }
   ],
   "schemas":[
      {
         "group":"platform-admins",
         "schema":".*",
         "owner":true
      },
      {
         "group":"metadata-users",
         "schema":"system.metadata",
         "owner":true
      },
      {
         "group":"platform-users",
         "schema":"system.runtime",
         "owner":true
      },
      {
         "catalog":".*",
         "schema":".*",
         "owner":false
      }
   ],
   "tables":[
      {
         "group":"platform-admins",
         "catalog":".*",
         "schema":".*",
         "privileges":[
            "SELECT",
            "INSERT",
            "DELETE",
            "OWNERSHIP",
            "GRANT_SELECT"
         ]
      },
      {
         "group":"metadata-users",
         "catalog":"system",
         "schema":"metadata",
         "privileges":[
            "SELECT",
            "INSERT",
            "DELETE",
            "OWNERSHIP",
            "GRANT_SELECT"
         ]
      },
      {
         "group":"platform-users",
         "catalog":"system",
         "schema":"runtime",
         "privileges":[
            "SELECT",
            "INSERT",
            "DELETE",
            "OWNERSHIP",
            "GRANT_SELECT"
         ]
      },
      {
         "group":".*",
         "catalog":".*",
         "schema":".*",
         "privileges":[
            
         ]
      }
   ]
}