{
  "defaultCacheCatalog": "hive_mv_tsr",
  "defaultCacheSchema": "cache",
  "defaultMaxImportDuration": "1m",
  "rules": [
    {
      "catalogName": "postgres",
      "schemaName": "public",
      "tableName": "customer",
      "refreshInterval": "90s",
      "gracePeriod": "5m"
    },
    {
      "catalogName": "postgres",
      "schemaName": "public",
      "tableName": "orders",
      "refreshInterval": "90s",
      "gracePeriod": "5m",
      "columns": [
        "orderkey",
        "totalprice"
      ]
    }
  ]
}