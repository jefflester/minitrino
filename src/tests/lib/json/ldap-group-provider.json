{
  "tests": [
    {
      "type": "query",
      "name": "Test clusteradmins group",
      "sql": "SELECT current_groups()",
      "trinoCliArgs": [
        "--server 'https://minitrino:8443'",
        "--user admin",
        "--truststore-path /etc/${CLUSTER_DIST}/tls/truststore.jks",
        "--truststore-password changeit",
        "--password"
      ],
      "env": {
        "TRINO_PASSWORD": "trinoRocks15"
      },
      "contains": [
        "clusteradmins"
      ]
    },
